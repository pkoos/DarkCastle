    "cmake.buildBeforeRun": true,
    "cmake.buildDirectory": "${workspaceFolder}/test/build",
    "cmakeExplorer.buildDir": "${workspaceFolder}/test/build",
    "cmakeExplorer.buildConfig": "${buildType}",
    "cmake.configureSettings": {
        "gtest_build_samples": "ON"
    },
    "C_Cpp.loggingLevel": "Debug",
    "cmakeExplorer.cmakeIntegration": true,
    "testExplorer.onReload": "reset",
    "testExplorer.onStart": "reset",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    // "testMate.cpp.log.logfile": "/home/koosp/projects/DarkCastle/test/testMate.log",
    "testMate.cpp.log.logpanel": true,
    "testMate.cpp.discovery.strictPattern": true,
    "testMate.cpp.test.advancedExecutables": [
        {
            "name": "main_test",
            "pattern": "${workspaceFolder}/test/build/main_test",
            "runTask": {
                "before": [
                    "cmake --build test --config Debug --target main_test -j 18 --clean-first --"
                ]
            }
        }
    ],
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c17"
}